    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "ajv": "^8.17.1",
